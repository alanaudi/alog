{
  "name": "root",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,svelte,md}\""
  },
  "devDependencies": {
    "prettier": "2.7.0",
    "rimraf": "^3.0.2",
    "turbo": "1.2.16"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.2.1"
}
